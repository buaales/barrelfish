/*
 * This interface is used in the e1000 driver for the device specific
 * communication between the device manager and the setup for the devif device
 * interface. The driver exposes the interface and the devif e1000 device
 * specific part connects to it
 */

interface zynqmp_cni_devif "zynqmp_cni devif communication interface" {
	message push_tx_msg(uint32 slot, String msg[256]);
	message read_rx_msg_request(uint32 slot);
	message read_rx_msg_response(String msg[256]);
	message check_controller_lifesign_request();
	message update_host_lifesign(uint32 lifesign);
};

